<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.RadioMenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.cryptorich.ForventetAfkastController">
   <children>
      <Slider fx:id="AfkastSlider" blockIncrement="1.0" layoutX="90.0" layoutY="360.0" prefHeight="20.0" prefWidth="340.0" />
      <Slider fx:id="StartbeløbSlider" blockIncrement="100.0" layoutX="90.0" layoutY="220.0" max="100000.0" min="1000.0" prefHeight="20.0" prefWidth="340.0" />
      <Text layoutX="100.0" layoutY="210.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Startbeløb">
         <font>
            <Font name="Arial Bold" size="16.0" />
         </font>
      </Text>
      <Label fx:id="StartbeløbLabel" layoutX="124.0" layoutY="243.0" text="0 kr">
         <font>
            <Font name="Arial Bold" size="16.0" />
         </font>
      </Label>
      <Text layoutX="100.0" layoutY="350.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Årligt afkast*">
         <font>
            <Font name="Arial Bold" size="16.0" />
         </font>
      </Text>
      <Slider fx:id="TidshorisontSlider" blockIncrement="1.0" layoutX="90.0" layoutY="500.0" prefHeight="20.0" prefWidth="340.0" />
      <Text layoutX="100.0" layoutY="490.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Tidshorisont">
         <font>
            <Font name="Arial Bold" size="16.0" />
         </font>
      </Text>
      <Label fx:id="AfkastLabel" layoutX="124.0" layoutY="381.0" text="0%">
         <font>
            <Font name="Arial Bold" size="16.0" />
         </font>
      </Label>
      <Label fx:id="TidshorisontLabel" layoutX="123.0" layoutY="523.0" text="0 år">
         <font>
            <Font name="Arial Bold" size="16.0" />
         </font>
      </Label>
      <Text layoutX="559.0" layoutY="237.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Portefølje værdi:">
         <font>
            <Font name="Arial Bold" size="24.0" />
         </font>
      </Text>
      <Label fx:id="PorteføljeLabel" layoutX="560.0" layoutY="263.0" prefHeight="32.0" prefWidth="192.0" text="0 kr">
         <font>
            <Font name="Charter Roman" size="24.0" />
         </font>
      </Label>
      <MenuBar prefHeight="29.0" prefWidth="800.0">
         <menus>
            <Menu mnemonicParsing="false" text="Oversigt">
               <items>
                  <RadioMenuItem mnemonicParsing="false" onAction="#switchToMinMainMenu" text="Min portfølje">
                     <toggleGroup>
                        <ToggleGroup fx:id="Menu" />
                     </toggleGroup>
                  </RadioMenuItem>
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Konto">
               <items>
                  <RadioMenuItem mnemonicParsing="false" onAction="#switchToKøbSælg" text="Køb/sælg" toggleGroup="$Menu" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Simulation">
               <items>
                  <RadioMenuItem mnemonicParsing="false" selected="true" text="Fibonacci Retracement" toggleGroup="$Menu" />
                  <SeparatorMenuItem mnemonicParsing="false" />
                  <RadioMenuItem mnemonicParsing="false" onAction="#switchToForventetAfkast" selected="true" text="Forventet afkast" toggleGroup="$Menu" />
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <Button layoutX="559.0" layoutY="322.0" mnemonicParsing="false" onAction="#Beregn" text="Beregn" />
      <Text layoutX="90.0" layoutY="105.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Beregn dit forventede afkast!">
         <font>
            <Font name="Gill Sans" size="33.0" />
         </font>
      </Text>
      <Text layoutX="92.0" layoutY="133.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Inklusiv renters rente effekt" />
   </children>
</AnchorPane>
